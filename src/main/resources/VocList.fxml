<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.SplitMenuButton?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<fx:root prefHeight="400.0" prefWidth="600.0" styleClass="mainVocList" stylesheets="@application.css" type="AnchorPane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <VBox fx:id="VBoxMain" prefHeight="400.0" prefWidth="606.0">
      <VBox fx:id="VBoxMenu" prefHeight="105.0" prefWidth="606.0">
         <HBox fx:id="HBoxMenuFrame" prefHeight="100.0" prefWidth="200.0">
            <VBox fx:id="VBoxMenuFrameLeft" prefHeight="100.0" prefWidth="458.0">

               <VBox prefHeight="200.0" prefWidth="100.0">
                  <children>
                     <TextField fx:id="SearchBarTextField" onAction="#textEntered" prefHeight="37.0" prefWidth="393.0" promptText="Suche eingeben" />
                     <Separator prefHeight="23.0" prefWidth="426.0" />
                     <ToggleButton fx:id="EditSwitch" onAction="#editSwitchPressed" mnemonicParsing="false" text="Vokabeln bearbeiten" />
                  </children>
               </VBox>
               <ButtonBar fx:id="ButtonBar" prefHeight="40.0" prefWidth="200.0">
                 <buttons>
                   <Button fx:id="DeleteButton" onAction="#deleteSelected" mnemonicParsing="false" text="Löschen" />
                     <Button fx:id="AddButton"  mnemonicParsing="false" text="Hinzufügen" />
                 </buttons>
               </ButtonBar>
            </VBox>
            <VBox fx:id="VBoxMenuFrameRight" prefHeight="100.0" prefWidth="174.0">
               <Button fx:id="SearchButton" mnemonicParsing="false" text="Suche" />
               <Separator prefHeight="16.0" prefWidth="162.0" />
               <SplitMenuButton fx:id="FilterButton" mnemonicParsing="false" text="Filter">
                 <items>
                   <MenuItem mnemonicParsing="false" text="Action 1" />
                   <MenuItem mnemonicParsing="false" text="Action 2" />
                 </items>
               </SplitMenuButton>
            </VBox>
         </HBox>
      </VBox>
      <VBox fx:id="VBoxList" prefHeight="306.0" prefWidth="606.0">
         <Separator prefHeight="15.0" prefWidth="606.0" />
         <HBox prefHeight="332.0" prefWidth="606.0">
            <HBox prefHeight="298.0" prefWidth="663.0">
               <TableView fx:id="VocTableList" layoutX="14.0" layoutY="75.0" prefHeight="298.0" prefWidth="630.0">
               </TableView>
            </HBox>
            <HBox prefHeight="235.0" prefWidth="50.0" />
         </HBox>
      </VBox>
   </VBox>
</fx:root>
